{
    "name": "adeyems/chuck-norris-jokes-laravel",
    "description": "Create random Chuck Norris's Jokes",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Adeyemo Adeyems",
            "email": "adeyems@gmail.com"
        }
